package ex1;

public class SwappingWithoutAuxiliaryVariable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 3;
		int y = 5;
		
		System.out.println("Before swapping: x = " + x + " und y = " + y);
		x = x + y;
		y = x - y;
		x = x - y;
		
		System.out.println("After swapping: x = " + x + " und y = " + y);

	}

}
